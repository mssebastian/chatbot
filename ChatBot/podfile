#podfile

source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'
use_frameworks!


def basic_pods
    pod 'Fabric'
    pod 'Crashlytics'
    pod 'Alamofire', '~> 4.0'
    pod 'ObjectMapper'
    pod 'AlamofireObjectMapper', '~> 4.0'
    pod 'NVActivityIndicatorView'
    pod 'SnapKit', '~> 3.0'
    pod 'Socket.IO-Client-Swift', '~> 8.1.2' # Or latest version

end

target 'ChatBot' do
	basic_pods
end
